{
    "rewrites": [
        { 
            "source": "/(.*)", 
            "destination": "/api/app" 
        }
    ],
    "functions": {
        "api/index.py": {
            "runtime": "python3.8",
            "includeFiles": [
                "app.py",
                "carros.py"
            ],
            "routes": [
                {
                    "src": "/carros",
                    "dest": "api/index.py"
                }
            ]
        }
    }
  }
  